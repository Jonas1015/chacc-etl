<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

    <id>icareetl</id>
    <name>IcareETL</name>
    <version>1.0.0-SNAPSHOT</version>
    <package>org.openmrs.module.mambaetl</package>
    <author>Jonas Mwambimbi</author>
    <description>This module is intended to flatten tables in icare for easier reporting</description>

    <activator>org.openmrs.module.icareetl.IcareETLActivator</activator>

    <require_version>1.11.6</require_version>

    <aware_of_modules>
        <aware_of_module>org.openmrs.module.webservices.rest</aware_of_module>
    </aware_of_modules>

    <!-- All message codes should start with icareetl.* -->
    <messages>
        <lang>en</lang>
        <file>messages.properties</file>
    </messages>
    <messages>
        <lang>fr</lang>
        <file>messages_fr.properties</file>
    </messages>
    <messages>
        <lang>es</lang>
        <file>messages_es.properties</file>
    </messages>

</module>